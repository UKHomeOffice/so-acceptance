  "dependencies": {
    "codeceptjs": "^2.6.11",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "express-session": "^1.17.2",
    "hof": "^19.6.1",
    "lodash": "^4.17.21",
    "webdriverio": "^7.9.1",
    "witch": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
